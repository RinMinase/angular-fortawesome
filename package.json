{
  "name": "@rinminase/ng-fortawesome",
  "version": "3.0.1",
  "license": "MIT",
  "author": "Rin Minase",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RinMinase/ng-fortawesome.git"
  },
  "scripts": {
    "build": "ng-packagr -p package.json && cp index.d.ts dist/index.d.ts",
    "release": "release-it --ci --no-increment --verbose"
  },
  "dependencies": {
    "@fortawesome/angular-fontawesome": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1"
  },
  "devDependencies": {
    "@angular/cli": "^9.0.3",
    "@angular/common": "^9.0.2",
    "@angular/compiler": "^9.0.2",
    "@angular/compiler-cli": "^9.0.2",
    "@angular/core": "^9.0.2",
    "@angular/platform-browser": "^9.0.2",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@types/node": "^13.7.4",
    "bestzip": "^2.1.5",
    "ng-packagr": "^9.0.1",
    "release-it": "^12.6.0",
    "rxjs": "^6.5.4",
    "ts-node": "^8.6.2",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5",
    "zone.js": "^0.10.2"
  },
  "peerDependencies": {
    "@angular/core": "^9.0.0"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "src/lib/public_api.ts",
      "flatModuleFile": "ng-fortawesome",
      "umdModuleIds": {
        "@fortawesome/angular-fontawesome": "@fortawesome/angular-fontawesome",
        "@fortawesome/fontawesome-svg-core": "@fortawesome/fontawesome-svg-core",
        "@fortawesome/free-brands-svg-icons": "@fortawesome/free-brands-svg-icons",
        "@fortawesome/free-solid-svg-icons": "@fortawesome/free-solid-svg-icons"
      }
    },
    "dest": "./dist",
    "whitelistedNonPeerDependencies": [
      "@fortawesome/angular-fontawesome",
      "@fortawesome/fontawesome-svg-core",
      "@fortawesome/free-brands-svg-icons",
      "@fortawesome/free-solid-svg-icons"
    ]
  },
  "release-it": {
    "git": {
      "addUntrackedFiles": false,
      "commit": false,
      "tag": false,
      "push": false,
      "requireCleanWorkingDir": false
    },
    "github": {
      "release": false
    },
    "npm": {
      "publish": true,
      "publishPath": "dist",
      "publishConfig": {
        "access": "public"
      }
    }
  }
}
